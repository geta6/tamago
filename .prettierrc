{
  "printWidth": 120,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "arrowParens": "always",
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "overrides": [
    {
      "files": ".*rc",
      "options": {
        "parser": "json-stringify"
      }
    },
    {
      "files": ".vscode/*.json",
      "options": {
        "parser": "json-stringify"
      }
    }
  ]
}
